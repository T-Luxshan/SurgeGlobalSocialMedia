{
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "unused-imports"
    ],
    "rules": {
        "unused-imports/no-unused-imports": "off",   
        "unused-imports/no-unused-vars": "off",       
        "no-unused-vars": "off",                      
        "react/prop-types": "off",                    
        "react/react-in-jsx-scope": "off" ,
        "react/no-unescaped-entities": "off"   
    }
  }
  